{
  "name": "meibo-system",
  "version": "0.1.0",
  "scripts": {
    "dev": "wrangler dev src/index.ts",
    "deploy": "wrangler deploy --minify src/index.ts",
    "wrangler": "wrangler",
    "drizzle-kit": "./node_modules/.bin/drizzle-kit",
    "generate": "./node_modules/.bin/drizzle-kit generate:sqlite",
    "migrations": "./node_modules/.bin/wrangler d1 migrations apply meibo_db",
    "migrations-local": "./node_modules/.bin/wrangler d1 migrations apply meibo_db --local",
    "lint": "run-p lint:*",
    "lint:prettier": "prettier --check ./",
    "lint:eslint": "eslint --ext .js,jsx,.ts,.tsx",
    "fix": "run-p fix:*",
    "fix:prettier": "prettier  --write ./",
    "fix:lint": "eslint --ext .js,jsx,.ts,.tsx --fix"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.1.11",
    "drizzle-orm": "^0.29.3",
    "hono": "^3.12.8",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-access": "^2.1.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.4",
    "wrangler": "^3.25.0"
  }
}
